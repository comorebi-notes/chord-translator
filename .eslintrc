{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "arrow-parens": 0,
    "indent": [2, 2, { "SwitchCase": 1 }],
    "jsx-a11y/anchor-has-content": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "jsx-a11y/label-has-for": 0,
    "class-methods-use-this": 0,
    "comma-dangle": 0,
    "no-var": 1,
    "no-fallthrough": 0,
    "default-case": 0,
    "func-names": 0,
    "object-curly-newline": 0,
    "prefer-destructuring": ["error", {"object": true, "array": false}],
    "space-before-function-paren": 0,
    "camelcase": 0,
    "jsx-quotes": 0,
    "semi": 0,
    "no-multi-spaces": 0,
    "no-else-return": 0,
    "quotes": 0,
    "key-spacing": 0,
    "no-class-assign": 0,
    "max-len": [
      "error", {
        "code": 120,
        "comments": 120
      }
    ]
  },
  "globals": {
    "window": true,
    "$": true,
    "before": true,
    "describe": true,
    "it": true
  }
}
